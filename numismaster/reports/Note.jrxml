		<queryString>
			<![CDATA[SELECT s.price,
	u.first_name,
	u.last_name,
	c.name
FROM `TB_COIN_USER_SALE` cus
INNER JOIN
	`TB_COIN_USER` cu on cu.id = cus.coin_user_id
INNER JOIN
	`TB_SALE` s on s.id = cus.sale_id
INNER JOIN
	`TB_USER` u on u.id = cu.user_id
INNER JOIN
	`TB_COIN` c on c.id = cu.coin_id
WHERE
s.id = $P{saleId}]]>
		</queryString>
		<field name="id" class="java.lang.String"/>
		<field name="productName" class="java.lang.String"/>
		<field name="buyerName" class="java.lang.String"/>
